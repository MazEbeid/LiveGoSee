<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="50dp"
    android:id="@+id/main_feed_fragment_toolbar"
    android:background="@color/transparent_white_hex_10">

<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_centerInParent="true">

    <ImageButton
        android:id="@+id/view_my_trips"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_gravity="center_vertical"
        android:background="@drawable/my_trips_icon" />

    <ImageButton
        android:id="@+id/adD_new_trip"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_gravity="center_vertical"
        android:background="@drawable/add_event_icon" />


    <ImageButton
        android:id="@+id/view_my_clips"
        android:layout_gravity="center_vertical"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="40dp"
        android:background="@drawable/my_clips_icon" />




</LinearLayout>


    </RelativeLayout>

